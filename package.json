{
  "name": "do-your-meme",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Arman Zohrabyan <arman.zohrabyan.96@gmail.com> (https://github.com/Arman-Zohrabyan)",
  "scripts": {
    "start": "node server.js",
    "start:all": "webpack -w & nodemon server.js",
    "watch": "webpack -w",
    "nodemon": "nodemon server.js",
    "eslint": "eslint ./src ./server ./browser --ext .jsx --ext .js",
    "eslint:fix": "eslint --fix ./src ./server ./browser --ext .jsx --ext .js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Arman-Zohrabyan/do-your-meme.git"
  },
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.1",
    "eslint": "^5.7.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "node-sass": "^4.9.4",
    "nodemon": "^1.12.5",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "geoip-lite": "^1.3.2",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "request-ip": "^2.1.3",
    "serialize-javascript": "^1.4.0"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "babel-plugin-transform-class-properties",
      "babel-plugin-transform-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jasmine": true
    },
    "rules": {
      "semi-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "semi": [
        2,
        "always"
      ],
      "max-len": [
        2,
        350,
        2
      ],
      "indent": [
        "error",
        2
      ],
      "no-param-reassign": [
        2,
        {
          "props": false
        }
      ],
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "never"
        }
      ],
      "quotes": [
        1,
        "single",
        "avoid-escape"
      ],
      "no-nested-ternary": 0,
      "no-tabs": 0,
      "no-uderscore-dangling": 0,
      "new-cap": 0,
      "no-underscore-dangle": 0,
      "prefer-destructuring": 0,
      "no-unused-vars": 1,
      "no-else-return": 1,
      "arrow-body-style": 1,
      "global-require": 1,
      "no-mixed-operators": 1,
      "newline-per-chained-call": 1,
      "space-infix-ops": 1,
      "import/extensions": {
        ".js": "never",
        ".json": "always",
        ".scss": "always"
      },
      "import/no-extraneous-dependencies": 0,
      "react/jsx-filename-extension": 0,
      "react/no-string-refs": 1,
      "react/no-array-index-key": 1,
      "react/jsx-uses-vars": 2,
      "react/jsx-uses-react": 2,
      "jsx-a11y/label-has-for": 1,
      "jsx-a11y/click-events-have-key-events": 1,
      "jsx-a11y/anchor-is-valid": 0,
      "jsx-a11y/no-noninteractive-element-interactions": 0,
      "jsx-a11y/no-static-element-interactions": 1,
      "react/jsx-boolean-value": 2,
      "react/jsx-closing-bracket-location": 2,
      "react/jsx-curly-spacing": [
        2,
        "never"
      ],
      "react/jsx-handler-names": 2,
      "react/jsx-key": 2,
      "react/jsx-max-props-per-line": [
        2,
        {
          "maximum": 3
        }
      ],
      "react/jsx-no-bind": [
        2,
        {
          "ignoreRefs": true,
          "allowBind": true,
          "allowArrowFunctions": true
        }
      ],
      "react/jsx-no-duplicate-props": 2,
      "react/jsx-no-undef": 2,
      "react/jsx-pascal-case": 2,
      "react/no-deprecated": 2,
      "react/no-did-mount-set-state": 0,
      "react/no-did-update-set-state": 0,
      "react/no-direct-mutation-state": 2,
      "react/no-is-mounted": 2,
      "react/no-multi-comp": 2,
      "react/no-unknown-property": 2,
      "react/prefer-es6-class": 2,
      "react/prop-types": 2,
      "react/react-in-jsx-scope": 2,
      "react/self-closing-comp": 2,
      "react/sort-comp": [
        2,
        {
          "order": [
            "lifecycle",
            "/^handle.+$/",
            "/^(get|set)(?!(InitialState$|DefaultProps$|ChildContext$)).+$/",
            "everything-else",
            "/^render.+$/",
            "render"
          ]
        }
      ]
    },
    "settings": {
      "react": {
        "version": "16.2"
      }
    }
  }
}
